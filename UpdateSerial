import serial
ser = serial.Serial('/dev/cu.usbmodem1421',9600)

import urllib2
import csv
import time
x=0
url ="https://graphite.sdsc.edu/render/?target=hosts.hpc.comet-02*.sensors.socket0.cpu0.temp&format=csv"
response = urllib2.urlopen(url)
cr = csv.reader(response)
for row in cr:
    if x % 2880 == 0:
        temp = row[2]
        print row
        ser.write(temp + ";")
        time.sleep(.5)
    x += 1
